[
  {
    "name": "Valid transaction",
    "requestBody": {
      "sender_id": "user1",
      "receiver_id": "user2",
      "amount": 100,
      "transaction_id": "tx1",
      "effective_date": "2023-06-01T10:00:00Z"
    },
    "expectedStatus": 200
  },
  {
    "name": "Non-existent sender",
    "requestBody": {
      "sender_id": "user10",
      "receiver_id": "user2",
      "amount": 100,
      "transaction_id": "tx2",
      "effective_date": "2023-06-01T10:00:00Z"
    },
    "expectedStatus": 500,
    "expectedError": "User does not exist"
  },
  {
    "name": "Non-existent receiver",
    "requestBody": {
      "sender_id": "user1",
      "receiver_id": "user20",
      "amount": 100,
      "transaction_id": "tx3",
      "effective_date": "2023-06-01T10:00:00Z"
    },
    "expectedStatus": 500,
    "expectedError": "User does not exist"
  },
  {
    "name": "Negative amount",
    "requestBody": {
      "sender_id": "user1",
      "receiver_id": "user2",
      "amount": -100,
      "transaction_id": "tx4",
      "effective_date": "2023-06-01T10:00:00Z"
    },
    "expectedStatus": 400,
    "expectedError": "リクエストデータが無効です"
  },
  {
    "name": "Insufficient balance",
    "requestBody": {
      "sender_id": "user1",
      "receiver_id": "user2",
      "amount": 10000,
      "transaction_id": "tx5",
      "effective_date": "2023-06-01T10:00:00Z"
    },
    "expectedStatus": 500,
    "expectedError": "Insufficient balance"
  },
  {
    "name": "Past effective date",
    "requestBody": {
      "sender_id": "user1",
      "receiver_id": "user2",
      "amount": 100,
      "transaction_id": "tx6",
      "effective_date": "2022-01-01T00:00:00Z"
    },
    "expectedStatus": 400,
    "expectedError": "effective_dateは現在時刻以降の値を指定してください"
  },
  {
    "name": "Duplicate transaction ID",
    "requestBody": {
      "sender_id": "user1",
      "receiver_id": "user2",
      "amount": 100,
      "transaction_id": "tx1",
      "effective_date": "2023-06-01T10:00:00Z"
    },
    "expectedStatus": 500,
    "expectedError": "Duplicate transaction"
  }
]